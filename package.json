{
  "name": "Eth2Dai",
  "version": "0.1.0",
  "private": true,
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "build:ci": "REACT_APP_ENV='test' node scripts/build.js",
    "test": "yarn tslint && node scripts/test.js --env=jsdom",
    "test:coverage": "yarn tslint && node scripts/test.js --env=jsdom --coverage",
    "test:fix": "yarn tslint:fix && CI=1 node scripts/test.js --env=jsdom",
    "tslint": "tslint --project .",
    "tslint:fix": "tslint --project . --format stylish --fix",
    "postinstall": "rm -f node_modules/web3/index.d.ts",
    "deploy:dev": "yarn build && surge --project ./build --domain http://eth2dai-dev.surge.sh/",
    "deploy:stage": "yarn build && surge --project ./build --domain http://eth2dai-staging.surge.sh/",
    "deploy:prod": "sh ./deploy-prod.sh",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "cypress:run": "cypress run",
    "cypress:run:ci": "cypress run --record --parallel",
    "cypress:dev": "NODE_ENV='development' cypress open",
    "tsc": "tsc -p ./tsconfig.prod.json --noEmit && tsc -p ./cypress/tsconfig.json --noEmit && tsc -p ./tsconfig.test.json --noEmit && tsc -p ./tsconfig.json --noEmit",
    "storybook:screenshots": "storybook-chrome-screenshot -p 9001 --inject-files ./.storybook/disable-animations.js",
    "super-ci": "super-ci"
  },
  "dependencies": {
    "@types/ramda": "^0.25.38",
    "await-exec": "^0.1.2",
    "bignumber.js": "^7.2.1",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "2.4.1",
    "classnames": "^2.2.6",
    "d3": "^5.7.0",
    "lodash": "^4.17.10",
    "mathjs": "^5.2.0",
    "moment": "^2.22.2",
    "normalize.css": "^8.0.1",
    "object-assign": "4.1.1",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "ramda": "^0.25.0",
    "react": "^16.3.2",
    "react-css-modules": "^4.7.3",
    "react-dev-utils": "^5.0.1",
    "react-dom": "^16.3.2",
    "react-faux-dom": "^4.2.0",
    "react-jazzicon": "^0.1.2",
    "react-modal": "^3.6.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-text-mask": "^5.4.1",
    "react-transition-group": "1.x",
    "resolve": "1.6.0",
    "rxjs": "^6.2.0",
    "rxjs-take-while-inclusive": "^2.1.0",
    "text-mask-addons": "^3.7.2",
    "web3": "0.20.6",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^1.0.0",
    "@storybook/addon-actions": "^3.4.10",
    "@storybook/addon-links": "^3.4.10",
    "@storybook/addons": "^3.4.10",
    "@storybook/react": "^3.4.10",
    "@types/bn.js": "^4.11.1",
    "@types/classnames": "^2.2.6",
    "@types/d3": "^5.0.0",
    "@types/jest": "^22.2.3",
    "@types/jest-each": "^0.3.2",
    "@types/lodash": "^4.14.109",
    "@types/mathjs": "^4.4.1",
    "@types/node": "^10.1.2",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "@types/react-faux-dom": "^4.1.1",
    "@types/react-modal": "^3.2.1",
    "@types/react-router": "^4.0.30",
    "@types/react-router-dom": "^4.3.0",
    "@types/react-text-mask": "^5.4.0",
    "@types/react-transition-group": "1.x",
    "@types/storybook__addon-actions": "^3.4.1",
    "@types/storybook__addon-info": "^3.4.1",
    "@types/storybook__react": "^3.0.9",
    "@types/webpack": "^4.4.9",
    "autoprefixer": "7.1.6",
    "babel-core": "^6.26.3",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "^6.26.0",
    "build-size-super-plugin": "^0.0.4",
    "css-loader": "0.28.7",
    "cypress": "^3.1.2",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "execa": "^1.0.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "0.11.2",
    "fork-ts-checker-webpack-plugin": "^0.4.6",
    "fs-extra": "3.0.1",
    "gh-pages": "^2.0.1",
    "html-webpack-plugin": "2.29.0",
    "http-server": "^0.11.1",
    "jest": "22.4.2",
    "jest-each": "^23.4.0",
    "node-sass": "^4.9.2",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "reg-simple-keygen-plugin": "^0.7.16",
    "reg-suit": "^0.7.17",
    "sass-loader": "^7.0.3",
    "source-map-loader": "^0.2.1",
    "storybook-chrome-screenshot": "1.3.1",
    "style-loader": "0.19.0",
    "super-ci": "^0.0.16",
    "surge": "^0.20.1",
    "sw-precache-webpack-plugin": "0.11.4",
    "truffle-privatekey-provider": "^1.0.0",
    "ts-jest": "22.0.1",
    "ts-loader": "^2.3.7",
    "ts-node": "^7.0.1",
    "tsconfig-paths-webpack-plugin": "^2.0.0",
    "tslint": "^5.7.0",
    "tslint-config-airbnb": "^5.9.2",
    "tslint-config-prettier": "^1.10.0",
    "tslint-react": "^3.2.0",
    "typescript": "^3.0.1",
    "typestrict": "^1.0.0",
    "typings-for-css-modules-loader": "^1.7.0",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "url-loader": "0.6.2",
    "web3-typescript-typings": "^0.10.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2"
  },
  "jest": {
    "coverageReporters": [
      "lcov"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "\\.stories\\.tsx$",
      "\\.d\\.ts$"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(j|t)s?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(j|t)s?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts|tsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json",
      "node",
      "mjs"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "./tsconfig.test.json"
      }
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
